{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": [true, "never-prefix"],
    "object-literal-sort-keys": false,
    "no-unused-expression": false,
    "no-namespace": [true, "allow-declarations"],
    "no-unused-variable": true
  }
}